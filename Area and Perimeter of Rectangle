#include<stdio.h>
int main()
{
 int Length, Breadth, Perimeter, Area;

 printf("Length: ");
 scanf("%d", &Length);

 printf("Breadth: ");
 scanf("%d", &Breadth);

 Perimeter =  2 * (Length + Breadth);
 Area = Length * Breadth;

 printf("The Perimeter of the Rectangle is %d\n", Perimeter);
 printf("The Area of the rectangle is %d\n", Area);

 return 0;
}
